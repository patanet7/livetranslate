# Production Environment Configuration
# Static build served through orchestration service

# API Base URL - direct connection to orchestration service
VITE_API_BASE_URL=http://localhost:3000

# WebSocket URL - direct connection to orchestration service at /api/websocket/connect
VITE_WS_BASE_URL=ws://localhost:3000

# Backend orchestration service
VITE_ORCHESTRATION_URL=http://localhost:3000

# Debug mode
VITE_ENABLE_DEBUG=false
VITE_LOG_LEVEL=error

# WebSocket configuration
VITE_WS_AUTO_RECONNECT=true
VITE_WS_MAX_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_HEARTBEAT_INTERVAL=30000
