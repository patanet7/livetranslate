{
  "name": "livetranslate-frontend-service",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm run type-check && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rimraf dist node_modules/.cache",
    "clean:deps": "rimraf node_modules pnpm-lock.yaml && pnpm install",
    "analyze": "pnpm run build && npx vite-bundle-analyzer dist",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "precommit": "lint-staged",
    "ci": "pnpm run lint && pnpm run type-check && pnpm run test && pnpm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.18",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.14.18",
    "@mui/x-charts": "^6.18.1",
    "@mui/x-date-pickers": "^6.18.1",
    "@reduxjs/toolkit": "^1.9.7",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.47.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.8.0",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "reactflow": "^11.11.4",
    "recharts": "^2.8.0",
    "rxjs": "^7.8.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/lodash": "^4.14.201",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^0.34.6"
  },
  "resolutions": {
    "glob": "^10.0.0",
    "rimraf": "^6.0.1",
    "inflight": "^1.0.6",
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "webpack"
      ]
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
