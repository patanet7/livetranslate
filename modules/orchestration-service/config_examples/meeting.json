{
  "description": "Meeting/Conference call - Optimized for online meetings with multiple speakers",
  "enabled_stages": [
    "vad",
    "voice_filter",
    "noise_reduction",
    "voice_enhancement",
    "agc",
    "compression",
    "limiter"
  ],
  "sample_rate": 16000,
  "quality": "high",
